---
import { SITE } from "~/config.mjs";
import { contentfulClient } from "../lib/contentful";
import { WebsiteDetails } from "../types";

const websiteDetails: WebsiteDetails = await contentfulClient.getEntry<WebsiteDetails>("295uP17ssbHUyTpzVBLmOF");
---

<span
  class="self-center ml-2 text-2xl md:text-xl font-bold whitespace-nowrap text-white hover:text-secondary dark:hover:text-gray-700"
>
  {websiteDetails.fields.websiteName}</span
>
